{
	"manifest_version": 2,
	"name": "Prayer Notifications",
	"version": "1.0",
	"icons":{
		"128": "icon.png"
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "Today's prayer timings"
	},
	"permissions": [
		"geolocation" , "notifications"
	]
}
